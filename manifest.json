{
  "name": "Remover publicidade do Anitube",
  "description": "Remove as publicidades que atrapalham na hora de assistir um anime.",
  "version": "1.0.0",
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "*://*/*"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Remover Publicidade Anitube"
  },
  "manifest_version": 2
}
